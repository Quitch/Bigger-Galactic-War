{
  "parserOptions": {
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "env": {
    "browser": true,
    "jquery": true,
    "amd": true
  },
  "settings": {
    "lodash": {
      "version": 3,
      "pragma": "_"
    }
  },
  "extends": ["eslint:recommended", "prettier", "plugin:lodash/v3"],
  "globals": {
    "requireGW": "readonly"
  },
  "plugins": ["lodash"],
  "rules": {
    "lodash/preferred-alias": [2, { "ignoreMethods": ["extend", "any"] }],
    "lodash/prefer-lodash-method": 0
  }
}
