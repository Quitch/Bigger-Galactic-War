{
  "parserOptions": {
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "env": {
    "browser": true,
    "jquery": true,
    "amd": true
  },
  "extends": ["eslint:recommended", "plugin:lodash/v3", "prettier"],
  "globals": {
    "requireGW": "readonly",
    "locUpdateDocument": "readonly"
  },
  "plugins": ["lodash"],
  "rules": {
    "lodash/preferred-alias": [2, { "ignoreMethods": ["extend", "any"] }],
    "lodash/prefer-lodash-method": 0
  }
}
